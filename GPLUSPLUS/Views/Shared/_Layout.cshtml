<body>
    <header id="header">
        <!--header-->
        <div class="header_top" style="position: fixed; width: 100%; z-index: 9999">
            <!--header_top-->
            <div class="container">
                <div class="row">
                    <div class="col-sm-6">
                        <div class="contactinfo">
                            @if (User.Identity.IsAuthenticated)
                            {
                                <ul class="nav nav-pills">
                                    <li>
                                        @using (Html.BeginForm("LogOff", "Account", FormMethod.Post, new { id = "logoutForm" }))
                                        {
                                            @Html.AntiForgeryToken()

                                            <a href="javascript:document.getElementById('logoutForm').submit()" class="btn btn-danger" style="height:39px;padding-top:10px">خــروج &nbsp; <span class="glyphicon glyphicon-log-out"></span></a>
                               
                                        }  

                                    </li>
                                    <li>
                                        <a href="@Url.Action("Manage", "Account")"  class = "btn btn-success">@User.Identity.Name &nbsp; <span class="glyphicon glyphicon-user"></span></a>

                                    </li>
                                </ul>
                            }
                            else
                            {
                                <ul class="nav nav-pills">
                                    <li>
                                        <a href="@Url.Action("register", "Account")"  class = "btn btn-warning">ثبت نام &nbsp; <span class="glyphicon glyphicon-plus-sign"></span></a>
                                    </li>
                                    <li>
                                        <a href="@Url.Action("login", "Account")"  class = "btn btn-warning">ورود &nbsp; <span class="glyphicon glyphicon-log-in"></span></a>
                                    </li>
                                </ul>
                            }
                        </div>
                    </div>
                    <div class="col-sm-6">
                        <div class="social-icons pull-right">
                            <h6 style="direction: rtl;">تمامی محصولات این سایت اورجینال می باشد.</h6>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!--/header_top-->

        <div class="header-middle">
            <!--header-middle-->
            <div class="container">
                <div class="row">
                    <div class="col-sm-4">
                        <div class="logo pull-left">
                        </div>

                    </div>
                    <div class="col-sm-8" style="direction: rtl; text-align: right;">
                    </div>
                </div>
            </div>
            <div id="custom-bootstrap-menu" class="navbar navbar-default " role="navigation" style="position: fixed; width: 100%; z-index: 999; margin-top: -5px">
                <div class="container-fluid">
                    <div class="navbar-header">
                        <a class="navbar-brand" href="/home" style="padding-bottom:0px;padding-top:0px;">
                            <img src="../../Images/newlogo.png" alt=""  height="60" />
                        </a>
                        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-menubuilder">
                            <span class="sr-only">Toggle navigation</span><span class="icon-bar"></span><span class="icon-bar"></span><span class="icon-bar"></span>
                        </button>
                    </div>
                    <div class="collapse navbar-collapse navbar-menubuilder">
                        <ul class="nav navbar-nav navbar-right" style="direction: rtl;">
                            <li style=" border-left:2px solid #00a99d;"><a href="/agents/agentregister">پیوستن به مراکز طرف قرارداد</a>
                            </li>
                            <li><a href="#">آرشیو نشریه اختصاصی</a>
                            </li>
                            <li><a href="/seller/home">مراکز طرف قرارداد</a>
                            </li>
                            <li><a href="/eshop/home">فروشگاه G&G</a>
                            </li>
                            <li class="active"><a href="/home">خانه</a>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
        <!--/header-middle-->
    </header>
    <!--/header-->





    @RenderSection("slider", required: false)
    @RenderBody()


    <footer id="footer" style="direction: rtl; text-align: right;">
        <!--Footer-->
        <div class="footer-top">
            <div class="container">
                <div class="row">
                    <div class="col-sm-6">
                        <div class="contactinfo" style="margin-top: 57px;">
                            <ul class="nav nav-pills" style="direction: ltr">
                                <li><a href="tel:+984432256020">044 - 32256020 <i class="fa fa-phone"></i></a></li>
                                <li><a href="mailto:Info@gtop.com?Subject=Hello%20again">Info@gtop.com <i class="fa fa-envelope"></i></a></li>
                            </ul>
                        </div>
                    </div>
                    <div class="col-sm-6">
                        <div class="companyinfo">
                            <h2>۷ روز هفته، ۲۴ ساعته پاسخگوی شما هستیم.</h2>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="footer-widget">
            <div class="container">
                <div class="row">
                    <div class="col-sm-4">
                        <div class="single-widget">
                            <h2>ما را در شبکه های اجتماعی دنبال کنید</h2>
                            <div class="social-icons">
                                <ul class="nav nav-pills nav-stacked">
